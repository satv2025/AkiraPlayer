{
    "compilerOptions": {
        "target": "ES2019",
        "lib": [
            "DOM",
            "ES2019"
        ],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "declaration": false,
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true
